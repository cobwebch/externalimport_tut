{
	"name": "cobweb/externalimport_tut",
	"type": "typo3-cms-extension",
	"description": "Tutorial for the External Import extension. Contains the manual and the necessary files.",
	"keywords": [
		"typo3",
		"extension",
		"composer.json"
	],
	"license": [
		"GPL-2.0-or-later"
	],
	"authors": [
		{
			"name": "Francois Suter",
			"role": "Developer",
			"homepage": "https://www.ideative.ch/"
		}
	],
	"autoload": {
		"psr-4": {
			"Cobweb\\ExternalimportTut\\": "Classes/"
		}
	},
	"require": {
		"php": "^7.4 || ^8.0 || ^8.1",
		"typo3/cms-core": "^11.5 || ^12.4",
		"cobweb/external_import": "^7.0 || dev-master",
		"cobweb/svconnector_csv": "^4.0",
		"cobweb/svconnector_feed": "^4.0",
		"georgringer/news": "^11.0"
	},
	"replace": {
		"typo3-ter/externalimport_tut": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "externalimport_tut"
		}
	}
}

